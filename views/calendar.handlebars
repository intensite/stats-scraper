<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>Date</th>
            <th>Heure</th>
            <th>Type</th>
            <th>Visiteur</th>
            <th>Local</th>
            <th>Lieu</th>
        </tr>
    </thead>
    <tbody>
        {{#each scheduleData}}
        <tr>
            <td>{{this.realDate}}</td>
            <td>{{this.date}}</td>
            <td>{{this.beginTime}}</td>
            <td>{{this.eventTypeName}}</td>
            <td>{{this.visitorTeamName}}</td>
            <td>{{this.localTeamName}}</td>
            <td>{{this.locationName}}</td>
        </tr>
        {{/each}}
    </tbody>

</table>

